{
  "name": "where-did-my-ram-go-extension",
  "version": "1.5.0",
  "description": "Track your tabs and see how much RAM they're consuming. Because sheep (RAM) can really add up!",
  "main": "create_store_package.js",
  "scripts": {
    "build": "node resources/create_store_package.js",
    "package": "node resources/create_store_package.js",
    "validate": "node -e \"require('./resources/create_store_package.js').validateManifest()\"",
    "shell-package": "bash resources/create_store_package.sh"
  },
  "keywords": [
    "chrome-extension",
    "tab-manager",
    "memory-tracker",
    "ram-monitor",
    "browser-extension"
  ],
  "author": "Tab Counter Extension",
  "license": "MIT",
  "devDependencies": {
    "archiver": "^6.0.2",
    "escomplex": "^1.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4"
  }
}
